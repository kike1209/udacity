
    

def rot13(s):
    abcd = 'abcdefghikjlmnopqrstuvwxyz'
    ABCD = abcd.upper()
    abcd_len = len(abcd)
    s2 = ''
    for i in range(len(s)):
        c = s[i]
        if c.isalpha():
            if c in abcd:
                c = abcd[(abcd.index(cc) + 13) % abcd_len]
            else:
                c = ABCD[(ABCD.index(cc) + 13) % abcd_len]
        s2 += c
    return s2
